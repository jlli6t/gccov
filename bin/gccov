import os
import sys

import gccov

from gccov.main import read_arg
from gccov.main import stream

if __name__ == '__main__':
	if len(sys.argv) == 1:
		sys.argv.append('-h')
		read_arg(sys.argv)

	stream.exe(sys.argv)
